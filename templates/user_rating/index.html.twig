{% extends 'base.html.twig' %}

{% block title %}Quiz rating{% endblock %}
{% block navigation %}
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item">
                <a class="nav-link" href="{{ path('mainpage') }}">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="{{ path('quizzes') }}">Quizzes</a>
            </li>
            <li class="nav-item">
            </li>
        </ul>
        <a class="nav-link btn btn-outline-info" href="{{ path('app_logout') }}">logout</a>
    </div>
    </nav>
{% endblock %}
{% block content %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    <div class="example-wrapper">
        {% for user in rating %}
            <div class="container-fluid">{{ user.user.username }}</div>
        {% endfor %}
    </div>
{% endblock %}
